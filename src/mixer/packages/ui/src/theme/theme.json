{
  "fontBase": {
    "family": "yellixregular, sans-serif",
    "size": {
      "xs": "14px",
      "sm": "15px",
      "md": "15px",
      "lg": "16px",
      "xl": "16px"
    },
    "lineHeight": "1.5",
    "weight": 500
  },
  "font": {
    "display10": {
      "size": "Max(90px, Min(240px, 16.666vw))",
      "lineHeight": "Max(100px, Min(220px, 15.27vw))"
    },
    "display20": {
      "size": "Max(90px, Min(200px, 13.88vw))",
      "lineHeight": "Max(100px, Min(200px, 13.88vw))"
    },
    "display30": {
      "size": "Max(60px, Min(160px, 11.11vw))",
      "lineHeight": "Max(72px, Min(160px, 11.11vw))"
    },
    "display40": {
      "size": "Max(60px, Min(120px, 8.33vw))",
      "lineHeight": "Max(72px, Min(130px, 9.02vw))"
    },
    "display50": {
      "size": "Max(40px, Min(90px, 6.25vw))",
      "lineHeight": "Max(48px, Min(100px, 6.94vw))"
    },
    "display60": {
      "size": "Max(40px, Min(72px, 5vw))",
      "lineHeight": "Max(48px, Min(90px, 6.25vw))"
    },
    "heading10": {
      "size": "Max(30px, Min(60px, 4.16vw))",
      "lineHeight": "Max(36px, Min(72px, 5vw))"
    },
    "heading20": {
      "size": "Max(24px, Min(48px, 3.33vw))",
      "lineHeight": "Max(36px, Min(60px, 4.16vw))"
    },
    "heading30": {
      "size": "Max(20px, Min(36px, 2.5vw))",
      "lineHeight": "Max(30px, Min(48px, 3.33vw))"
    },
    "heading40": {
      "size": "Max(16px, Min(30px, 2.08vw))",
      "lineHeight": "Max(24px, Min(40px, 2.77vw))"
    },
    "paragraph10": {
      "size": "Max(30px, Min(36px, 2.5vw))",
      "lineHeight": "Max(36px, Min(48px, 3.33vw))"
    },
    "paragraph20": {
      "size": "Max(24px, Min(24px, 1.66vw))",
      "lineHeight": "Max(32px, Min(36px, 2.5vw))"
    },
    "paragraph30": {
      "size": "Max(16px, Min(18px, 1.25vw))",
      "lineHeight": "Max(24px, Min(30px, 1.25vw))"
    },
    "paragraph40": {
      "size": "Max(14px, Min(16px, 1.11vw))",
      "lineHeight": "Max(20px, Min(24px, 1.66vw))"
    },
    "paragraph50": {
      "size": "Max(14px, Min(14px, 0.97vw))",
      "lineHeight": "Max(20px, Min(20px, 1.38vw))"
    },
    "label10": {
      "size": "Max(16px, Min(18px, 1.25vw))",
      "lineHeight": "Max(16px, Min(18px, 1.25vw))"
    },
    "label20": {
      "size": "Max(14px, Min(16px, 1.11vw))",
      "lineHeight": "Max(14px, Min(16px, 1.11vw))"
    },
    "label30": {
      "size": "Max(12px, Min(14px, 0.97vw))",
      "lineHeight": "Max(12px, Min(14px, 0.97vw))"
    }
  },
  "grid": {
    "default": {
      "columns": 12,
      "size": "1fr",
      "columnGap": {
        "xs": "20px",
        "sm": "30px",
        "md": "30px",
        "lg": "30px",
        "xl": "30px"
      },
      "rowGap": {
        "xs": "0",
        "sm": "0",
        "md": "0",
        "lg": "0",
        "xl": "0"
      }
    },
    "full": {
      "columns": 12,
      "size": "1fr",
      "columnGap": {
        "xs": "0",
        "sm": "0",
        "md": "0",
        "lg": "0",
        "xl": "0"
      },
      "rowGap": {
        "xs": "0",
        "sm": "0",
        "md": "0",
        "lg": "0",
        "xl": "0"
      }
    }
  },
  "mediaQuery": {
    "xs": "0",
    "sm": "768px",
    "md": "1024px",
    "lg": "1440px",
    "xl": "1920px"
  },
  "container": {
    "default": {
      "maxWidth": {
        "xs": "calc(100% - var(--grid-column-gap) * 2)",
        "sm": "calc(100% - var(--grid-column-gap) * 4)",
        "md": "calc(100% - var(--grid-column-gap) * 4)",
        "lg": "calc(100% - var(--grid-column-gap) * 4)",
        "xl": "calc(1870px - var(--grid-column-gap) * 4)"
      }
    },
    "fluid": {
      "maxWidth": {
        "xs": "calc(100% - var(--grid-column-gap) * 2)",
        "sm": "calc(100% - var(--grid-column-gap) * 2)",
        "md": "calc(100vw - var(--grid-column-gap) * 2)",
        "lg": "calc(100vw - var(--grid-column-gap) * 2)",
        "xl": "calc(100vw - var(--grid-column-gap) * 2)"
      }
    }
  },
  "spacing": {
    "xs": "2.083vw",
    "sm": "4.166vw",
    "md": "8.333vw",
    "lg": "16.666vw",
    "xl": "24.999vw"
  },
  "margin": {
    "xs": "15px",
    "sm": "30px",
    "md": "45px",
    "lg": "60px",
    "xl": "90px"
  },
  "button": {
    "size": {
      "xs": "0.7rem",
      "sm": "0.8rem",
      "md": "1rem",
      "lg": "1.25rem",
      "xl": "1.45rem"
    },
    "borderRadius": "3em"
  },
  "aside": {
    "width": "33.333%",
    "gap": {
      "sm": "60px",
      "md": "120px"
    }
  },
  "form": {
    "default": {
      "backgroundColor": "transparent",
      "backgroundColorDisabled": "var(--color-neutral-200)",
      "backgroundColorInfo": "var(--color-warning-100)",
      "backgroundColorError": "var(--color-warning-100)",
      "color": "inherit",
      "colorHover": "var(--color-neutral-900)",
      "colorDisabled": "var(--color-neutral-700)",
      "colorInfo": "var(--color-warning-900)",
      "colorError": "var(--color-warning-900)",
      "border": "0 solid transparent",
      "borderWidth": "2px",
      "borderColor": "var(--color-neutral-800)",
      "borderColorHover": "var(--color-neutral-900)",
      "borderColorDisabled": "var(--color-neutral-300)",
      "borderColorInfo": "var(--color-warning-500)",
      "borderColorError": "var(--color-warning-500)",
      "borderRadius": "var(--border-radius)",
      "outline": "2px solid transparent",
      "outlineColorFocus": "var(--color-cyan-200)",
      "outlineColorActive": "var(--color-cyan-500)",
      "outlineColorError": "var(--color-warning-500)",
      "outlineOffset": "2px",
      "transition": "border 150ms ease-in 0s, outline-color 150ms ease-in 0s, color 200ms ease-out 0s, box-shadow 200ms ease 0s",
      "field": {
        "gap": "0.5rem"
      },
      "control": {
        "padding": "0.8em",
        "fontSize": "1.125rem",
        "fontWeight": "inherit",
        "lineHeight": "1.6"
      },
      "label": {
        "padding": "0",
        "fontSize": "0.9rem",
        "fontWeight": "500",
        "lineHeight": "1.2"
      },
      "error": {
        "margin": "0.5em 0",
        "padding": "0.2em 0.4em",
        "fontSize": "0.9rem",
        "fontWeight": "500",
        "lineHeight": "1.2",
        "border": "none",
        "backgroundColor": "var(--form-background-color-error)",
        "color": "var(--form-color-error)"
      }
    },
    "negative": {
      "backgroundColorDisabled": "var(--color-neutral-800)",
      "colorHover": "var(--color-neutral-200)",
      "colorDisabled": "var(--color-neutral-600)",
      "borderColor": "var(--color-neutral-700)",
      "borderColorHover": "var(--color-neutral-600)",
      "borderColorDisabled": "var(--color-neutral-700)",
      "outlineColorFocus": "var(--color-cyan-800)",
      "outlineColorActive": "var(--color-cyan-500)",
      "borderWidth": "0 0 2px 0",
      "borderRadius": "0",
      "outlineOffset": "0"
    }
  },
  "card": {
    "shadow": "0 20px 20px -10px rgba(0,0,0,0.2)"
  },
  "backdrop": {
    "opacity": 0.5
  },
  "border": {
    "radius": "0.5em"
  },
  "shadow": {
    "xs": "0 0.1rem 0.4rem -0.1rem rgba(0, 0, 0, 0.2)",
    "sm": "0 0.3rem 0.8rem -0.2rem rgba(0, 0, 0, 0.3)",
    "md": "0 0.6rem 1.6rem -0.3rem rgba(0, 0, 0, 0.4)",
    "lg": "0 0.9rem 3.2rem -0.4rem rgba(0, 0, 0, 0.5)",
    "xl": "0 1.2rem 6.4rem -0.5rem rgba(0, 0, 0, 0.6)"
  },
  "transition": {
    "smooth": "all 0.4s cubic-bezier(0.73, 0.01, 0.24, 1)"
  },
  "scrollbar": {
    "width": "8px"
  },
