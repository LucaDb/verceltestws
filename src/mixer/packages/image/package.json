{
  "name": "@websolute/bom-mixer-image",
  "version": "1.0.6",
  "private": true,
  "description": "Mixer Image module of the BOM Repository",
  "keywords": [
    "bom",
    "mixer",
    "image"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/websolutespa/bom.git"
  },
  "scripts": {
    "lint": "eslint *.ts*",
    "test": "jest",
    "test-watch": "npm test -- --watch",
    "coverage:integration": "npm run test -- --coverage",
    "coverage": "run-s -c --silent coverage:*",
    "compile": "tsup ./src/index.ts --format esm,cjs --dts",
    "prepack": "bom prepack",
    "postpack": "bom postpack"
  },
  "dependencies": {},
  "peerDependencies": {
    "@websolutespa/bom-core": "*",
    "next": ">= 14.1.4",
    "sharp": ">= 0.30.4"
  },
  "devDependencies": {
    "@types/sharp": "^0.30.4",
    "@websolutespa/bom-cli": "*",
    "@websolutespa/test": "*",
    "@websolutespa/tsconfig": "*",
    "eslint": "^8.56.0",
    "eslint-config-websolute": "*",
    "npm-run-all": "^4.1.5",
    "sharp": "0.30.4",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "prepack": {
    "types": "./dist/index.d.ts",
    "main": "./dist/index.js",
    "source": "./src/index.ts"
  },
  "postpack": {
    "types": "./src/index.ts",
    "main": "./src/index.ts"
  },
  "types": "./src/index.ts",
  "main": "./src/index.ts"
}
